{
  "manifest_version": 3,
  "name": "Memories - Save Link",
  "version": "1.0",
  "description": "Right-click to save any website to your Memories.",
  "permissions": ["contextMenus"],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
